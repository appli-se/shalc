procedure testLoops(integer n)
begin
    integer i;
    i = 0;
    while i < n begin
        i = i + 1;
    end;
    for(i = 0; i < 3; i = i + 1)
    begin
        n = n + i;
    end;
    if n > 10 then
    begin
        n = n - 1;
    end;
    switch n of
    case 0:
        n = 1;
    case 1:
        n = 2;
    otherwise
        n = 3;
    end;
end;
